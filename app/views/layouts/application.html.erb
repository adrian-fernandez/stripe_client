<!DOCTYPE html>
<html>
<head>
  <title>StripeClient</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="header">
  <h1><%= yield :title %></h1>
  <%= yield :subtitle %>
  <hr>
</div>

<div id="body">
<%= yield %>
</div>

<div id="footer">
  <hr>
  <%= yield :footer %>
</div>

</body>
</html>
